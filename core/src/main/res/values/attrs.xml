<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="src" format="reference" />
    <attr name="title" format="string" />
    <attr name="tint" format="color|reference" />
    <attr name="textAppearance" format="reference" />

    <declare-styleable name="ToolbarIconButton">
        <attr name="src" />
    </declare-styleable>

    <declare-styleable name="BaseToolbar">
        <attr name="back_icon" format="reference" />
        <attr name="title" />
        <attr name="tint" />
        <attr name="textAppearance" />
    </declare-styleable>

    <declare-styleable name="EmptyStubView">
        <attr name="src" />
        <attr name="srcSide" format="dimension" />
        <attr name="title" />
        <attr name="description" format="reference" />
        <attr name="titleTextAppearance" format="reference" />
        <attr name="descriptionTextAppearance" format="reference" />
    </declare-styleable>

</resources>
